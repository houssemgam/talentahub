<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#FFFF00">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/looogo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/looogo" />
        <ImageView
            android:id="@+id/loo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/loo" />


        <EditText
            android:id="@+id/titreproEditText"
            android:layout_width="match_parent"
            android:layout_height="67dp"

            android:hint="titre pro " />

        <EditText
            android:id="@+id/emailEditText"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:hint="Adresse e-mail" />

        <EditText
            android:id="@+id/teamtalentEditText"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:hint="team talent" />

        <EditText
            android:id="@+id/carteproEditText"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:hint="carte pro" />

        <EditText
            android:id="@+id/typetalentEditText"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:hint="type talent" />

        <EditText
            android:id="@+id/besoinmaterielleEditText"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:hint="besoin materielle" />

        <Button
            android:id="@+id/mybutton"
            android:layout_width="match_parent"
            android:layout_height="60dp"

            android:text="ajouter talent" />

    </LinearLayout>


</LinearLayout>