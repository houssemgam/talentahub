{
  "openapi": "3.0.0",
  "info": {
    "title": "User API",
    "description": "API de gestion des utilisateurs",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:5002"
    }
  ],
  
  
  "paths": {
    "/talent/getTalent/{id}": {
      "get": {
        "summary": "Obtenir les informations d'un talent",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "Identifiant du talent",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Informations du talent récupérées avec succès",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Talent"
                }
              }
            }
          },
          "401": {
            "description": "Non autorisé",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "404": {
            "description": "Talent non trouvé",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "description": "Erreur interne du serveur",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Talent": {
        "type": "object",
        "properties": {
          
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "Message d'erreur"
          }
        }
      }
    }
  }
}